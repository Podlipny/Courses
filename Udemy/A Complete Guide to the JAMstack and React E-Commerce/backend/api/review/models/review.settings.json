{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "name": "review"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "text": {
      "type": "text",
      "required": false
    },
    "rating": {
      "type": "decimal",
      "required": true
    },
    "product": {
      "via": "reviews",
      "model": "product"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "reviews"
    }
  }
}
